{
    "name": "reacttemplate",
    "author": "Dennetix",
    "license": "MIT",
    "version": "1.0.0",
    "description": "A React Template using TypeScript",
    "main": "index.js",
    "scripts": {
        "build": "webpack-cli --display-error-details",
        "buildw": "webpack-cli -w --display-error-details",
        "start": "node ./dist/static/server.bundle.js",
        "startw": "nodemon ./dist/static/server.bundle.js",
        "test": "jest",
        "lint": "tslint --project tsconfig.json --config tslint.json"
    },
    "jest": {
        "transform": {
            "^.+\\.tsx?$": "ts-jest"
        },
        "setupFiles": [
            "<rootDir>/src/test/testSetup.ts"
        ],
        "testRegex": ".*\\.test\\.tsx?$",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
            "\\.(css|less)$": "identity-obj-proxy"
        },
        "moduleDirectories": [
            "node_modules",
            "src"
        ]
    },
    "devDependencies": {
        "@types/body-parser": "^1.16.8",
        "@types/compression": "0.0.35",
        "@types/enzyme": "^3.1.9",
        "@types/enzyme-adapter-react-16": "^1.0.2",
        "@types/express": "^4.11.1",
        "@types/jest": "^22.2.0",
        "@types/morgan": "^1.7.35",
        "@types/node": "^9.4.7",
        "@types/react": "^16.0.40",
        "@types/react-dom": "^16.0.4",
        "@types/supertest": "^2.0.4",
        "css-loader": "^0.28.10",
        "enzyme": "^3.3.0",
        "enzyme-adapter-react-16": "^1.1.1",
        "identity-obj-proxy": "^3.0.0",
        "jest": "^22.4.2",
        "nodemon": "^1.17.1",
        "postcss-cssnext": "^3.1.0",
        "postcss-import": "^11.1.0",
        "postcss-loader": "^2.1.1",
        "style-loader": "^0.20.3",
        "supertest": "^3.0.0",
        "ts-jest": "^22.4.1",
        "ts-loader": "^4.0.1",
        "tslint": "^5.9.1",
        "tslint-config-airbnb": "^5.7.0",
        "tslint-react": "^3.5.1",
        "typescript": "^2.7.2",
        "typings-for-css-modules-loader": "^1.7.0",
        "webpack": "^4.1.1",
        "webpack-cli": "^2.0.11"
    },
    "dependencies": {
        "body-parser": "^1.18.2",
        "compression": "^1.7.2",
        "ejs": "^2.5.7",
        "express": "^4.16.3",
        "morgan": "^1.9.0",
        "react": "^16.2.0",
        "react-dom": "^16.2.0"
    }
}
